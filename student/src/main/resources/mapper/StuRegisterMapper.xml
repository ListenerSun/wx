<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sqt.edu.student.mapper.StuRegisterMapper">

     <!--根据条件查询补课班级信息列表-->
     <select id="queryByPhoneAndName" resultType="com.sqt.edu.student.dto.resp.StuRegisterInfoVo">
          select sri.student_name,
                 sri.phone,
                 sri.address,
                 sri.class_info_id,
                 ci.class_name,
                 ci.grade,
                 sri.subjects,
                 sri.year
          from stu_register_info sri
         left join class_info ci on sri.class_info_id = ci.id
         <if test="">

         </if>
     </select>
</mapper>